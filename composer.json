{
    "name": "cutedogspark/laravel-push-notification",
    "description": "Laravel package to send push notifications to mobile devices (apns, gcm, fcm)",
    "keywords": ["apns", "gcm", "fcm", "firebase", "push", "notification", "laravel"],
    "authors": [
        {
            "name": "DaviBenNun",
            "email": "davi@andradenunes.org"
        },
        {
             "name": "Chen Ming-Jui",
             "email": "cutedogspark@gmail.com"
        }  
    ],
    "require": {
        "php": ">=5.3.0",
        "illuminate/support": "5.*",
        "cutedogspark/notification-pusher": "2.*"
    },
    "autoload": {
        "psr-0": {
            "Davibennun\\LaravelPushNotification": "src/"
        }
    },
    "minimum-stability": "dev",
    "repositories": {
        "packagist": {
            "type": "composer",
            "url": "https://packagist.org"
        }
    }
}
